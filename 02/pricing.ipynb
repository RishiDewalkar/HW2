    "# pricing.py\n",
    "\n",
    "def get_net_price(price, tax_rate, discount=0):\n",
    "    return price * (1 + tax_rate) * (1-discount)\n",
    "\n",
    "\n",
    "def get_tax(price, tax_rate=0):\n",
    "    return price * tax_rate"
